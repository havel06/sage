{
	"repeatable": true,
	"events": [
		{
			"type": "echo",
			"parameters": {
				"message": "Punch begins!"
			}
		},
		{
			"type": "select_target",
			"parameters": {
				"target_type": "enemy"
			}
		},
		{
			"type": "echo",
			"parameters": {
				"message": "Punch selected target!"
			}
		},
		{
			"type": "delay",
			"parameters": {
				"seconds": 0.5
			}
		},
		{
			"type": "change_target_hp",
			"parameters": {
				"amount": -5
			}
		},
		{
			"type": "echo",
			"parameters": {
				"message": "Punch ended!"
			}
		}
	]
}
