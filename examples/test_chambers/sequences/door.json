{
	"repeatable": true,
	"events": [
		{
			"type": "play_sound",
			"parameters": {
				"sound": "audio/door.wav"
			}
		},
		{
			"type": "show_gui",
			"parameters": {
				"widget": "gui/map_transition.json"
			}
		},
		{
			"type": "delay",
			"parameters": {
				"seconds": 0.3
			}
		},
		{
			"type": "change_map",
			"parameters": {
				"map": "$map"
			}
		},
		{
			"type": "teleport_player",
			"parameters": {
				"x": "$x",
				"y": "$y"
			}
		},
		{
			"type": "rotate_player",
			"parameters": {
				"direction": "down"
			}
		},
		{
			"type": "hide_gui",
			"parameters": {}
		}
	]
}
