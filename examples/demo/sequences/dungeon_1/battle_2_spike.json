{
	"repeatable": true,
	"events": [
		{
			"type": "teleport_entity",
			"parameters": {
				"entity": "$entity",
				"x": "$x",
				"y": "$y"
			}
		},
		{
			"type": "change_sprite",
			"parameters": {
				"entity": "$entity",
				"sprite": {
					"frame_time": 0.25,
					"frames": [
						{
							"texture": "textures/spikes.png",
							"position_x": 0,
							"position_y": 0,
							"size_x": 16,
							"size_y": 16
						},
						{
							"texture": "textures/spikes.png",
							"position_x": 16,
							"position_y": 0,
							"size_x": 16,
							"size_y": 16
						},
						{
							"texture": "textures/spikes.png",
							"position_x": 32,
							"position_y": 0,
							"size_x": 16,
							"size_y": 16
						},
						{
							"texture": "textures/spikes.png",
							"position_x": 48,
							"position_y": 0,
							"size_x": 16,
							"size_y": 16
						}
					]
				}
			}
		},
		{
			"type": "delay",
			"parameters": {
				"seconds": 0.3
			}
		},
		{
			"type": "play_sound",
			"parameters": {
				"sound": "audio/spike.wav"
			}
		},
		{
			"type": "delay",
			"parameters": {
				"seconds": 0.7
			}
		},
		{
			"type": "change_sprite",
			"parameters": {
				"entity": "$entity",
				"sprite": {
					"texture": "textures/spikes.png",
					"position_x": 64,
					"position_y": 0,
					"size_x": 16,
					"size_y": 16
				}
			}
		}
	]
}
